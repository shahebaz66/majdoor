
<%- include("../homepartials/headers") %>

<div class="container">
<div class="row">

<% for(var i=0; i< listOfMajdoor.length; i++){ %>



    <div class="pricing-column col-lg-3 col-md-6">
      <div class="card">
        <div class="card-header">


          <h3><%= listOfMajdoor[i].category %></h3>
        </div>
        <div class="card-body">

          <p><strong>Name:</strong> <%= listOfMajdoor[i].name %></p>
          <p><strong>Gender:</strong><%= listOfMajdoor[i].gender%></p>
          <form  class="form-group" action="/majdoor/list" method="post">

            <input type="hidden" name="postId" value="<%= postId %>">
            <input type="hidden" name="category" value="<%= option %>">
            <input type="hidden" name="location" value="<%= location %>">
            <input type="hidden" name="majdoorid" value=<%= listOfMajdoor[i]._id %>>
            <button class="btn btn-lg btn-dark btn-block" type="submit">Confirm</button>
            </form>


        </div>
      </div>
    </div>


<% } %>
</div>
</div>

<%- include("../homepartials/footer") %>
